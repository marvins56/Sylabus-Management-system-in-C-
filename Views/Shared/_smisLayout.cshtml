
<!DOCTYPE html>
<html lang="en">


<!-- index.html  21 Nov 2019 03:44:50 GMT -->
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>SMIS @ViewBag.Title</title>
    @Styles.Render("~/Content/assets/css")
    @Scripts.Render("~/Content/assets/bundles/")
    <!-- General CSS Files -->
    <link rel="stylesheet" href="~/Content/assets/css/app.min.css">
    <link rel="stylesheet" href="~/Content/assets/bundles/izitoast/css/iziToast.min.css">
    <!-- Template CSS -->
    <link rel="stylesheet" href="~/Content/assets/bundles/bootstrap-social/bootstrap-social.css">

    <link rel="stylesheet" href="~/Content/assets/css/style.css">
    <link rel="stylesheet" href="~/Content/assets/css/components.css">
    <link rel="stylesheet" href="~/Content/assets/bundles/prism/prism.css">
    <!-- Custom style CSS -->
    <link rel="stylesheet" href="~/Content/assets/css/custom.css">
    <link rel='shortcut icon' type='image/x-icon' href='~/Content/assets/img/favicon.ico' />
</head>

<body>
    <div class="loader"></div>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>
            <nav class="navbar navbar-expand-lg main-navbar sticky">
                <div class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li>
                            <a href="#" data-toggle="sidebar" class="nav-link nav-link-lg
									collapse-btn"> <i data-feather="align-justify"></i></a>
                        </li>
                        <li>
                            <a href="#" class="nav-link nav-link-lg fullscreen-btn">
                                <i data-feather="maximize"></i>
                            </a>
                        </li>

                    </ul>
                </div>
                @Html.Partial("_LoginPartial")
            </nav>
            <div class="main-sidebar sidebar-style-2">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="@Url.Action("index","Class")">
                            <img alt="image" src="~/Content/assets/img/logo.png" class="header-logo" /> <span class="logo-name">SMIS</span>
                        </a>
                    </div>
                    <ul class="sidebar-menu">
                        <li class="menu-header">Main</li>
                        <li class="dropdown active">
                            <a href="#" class="nav-link"><i data-feather="monitor"></i><span>Dashboard</span></a>
                        </li>

                        <li class="dropdown">
                            <a href="#" class="menu-toggle nav-link has-dropdown">
                                <i class="fas fa-cog"></i></i><span>Setting</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "Users")">
                                        <i class="material-icons">account_box</i> <span class="icon-name">Users</span>

                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "Students")">
                                        <i class="material-icons">account_box</i> <span class="icon-name">Students</span>

                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "AspNetRoles")">
                                        <i class="material-icons">account_box</i> <span class="icon-name">Roles</span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="#">
                                        <i class="material-icons">account_box</i> <span class="icon-name">UserRoles</span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "Topics")">
                                        <i data-feather="copy">
                                        </i><span>
                                            topics
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "SubTopics")">
                                        <i data-feather="copy">
                                        </i><span>
                                            subtopic
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "Marks")">
                                        <i data-feather="copy">
                                        </i><span>
                                            Marks
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "Subject")">
                                        <i data-feather="copy">
                                        </i><span>
                                            subjects
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "Class")">
                                        <i data-feather="copy">
                                        </i><span>
                                            Classes
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "subject_class")">
                                        <i data-feather="copy">
                                        </i><span>
                                            Subject_Class
                                        </span>
                                    </a>
                                </li>


                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "Weeks")">
                                        <i class="material-icons">date_range</i> <span class="icon-name">Weeks</span>
                                    </a>
                                </li>
                                <li>
                                    <a class="nav-link" href="@Url.Action("Index", "Years")">
                                        <i class="material-icons">date_range</i> <span class="icon-name">Years</span>
                                    </a>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </aside>
            </div>

            <div class="main-content">
                <section class="section">
                    @{
                        Html.RenderPartial("_Notifications");
                    }

                    @RenderBody();

                </section>
                <div class="settingSidebar">
                    <a href="javascript:void(0)" class="settingPanelToggle">
                        <i class="fa fa-spin fa-cog"></i>
                    </a>
                    <div class="settingSidebar-body ps-container ps-theme-default">
                        <div class=" fade show active">
                            <div class="setting-panel-header">
                                Setting Panel
                            </div>
                            <div class="p-15 border-bottom">
                                <h6 class="font-medium m-b-10">Select Layout</h6>
                                <div class="selectgroup layout-color w-50">
                                    <label class="selectgroup-item">
                                        <input type="radio" name="value" value="1" class="selectgroup-input-radio select-layout" checked>
                                        <span class="selectgroup-button">Light</span>
                                    </label>
                                    <label class="selectgroup-item">
                                        <input type="radio" name="value" value="2" class="selectgroup-input-radio select-layout">
                                        <span class="selectgroup-button">Dark</span>
                                    </label>
                                </div>
                            </div>
                            <div class="p-15 border-bottom">
                                <h6 class="font-medium m-b-10">Sidebar Color</h6>
                                <div class="selectgroup selectgroup-pills sidebar-color">
                                    <label class="selectgroup-item">
                                        <input type="radio" name="icon-input" value="1" class="selectgroup-input select-sidebar">
                                        <span class="selectgroup-button selectgroup-button-icon" data-toggle="tooltip"
                                              data-original-title="Light Sidebar"><i class="fas fa-sun"></i></span>
                                    </label>
                                    <label class="selectgroup-item">
                                        <input type="radio" name="icon-input" value="2" class="selectgroup-input select-sidebar" checked>
                                        <span class="selectgroup-button selectgroup-button-icon" data-toggle="tooltip"
                                              data-original-title="Dark Sidebar"><i class="fas fa-moon"></i></span>
                                    </label>
                                </div>
                            </div>
                            <div class="p-15 border-bottom">
                                <h6 class="font-medium m-b-10">Color Theme</h6>
                                <div class="theme-setting-options">
                                    <ul class="choose-theme list-unstyled mb-0">
                                        <li title="white" class="active">
                                            <div class="white"></div>
                                        </li>
                                        <li title="cyan">
                                            <div class="cyan"></div>
                                        </li>
                                        <li title="black">
                                            <div class="black"></div>
                                        </li>
                                        <li title="purple">
                                            <div class="purple"></div>
                                        </li>
                                        <li title="orange">
                                            <div class="orange"></div>
                                        </li>
                                        <li title="green">
                                            <div class="green"></div>
                                        </li>
                                        <li title="red">
                                            <div class="red"></div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="p-15 border-bottom">
                                <div class="theme-setting-options">
                                    <label class="m-b-0">
                                        <input type="checkbox" name="custom-switch-checkbox" class="custom-switch-input"
                                               id="mini_sidebar_setting">
                                        <span class="custom-switch-indicator"></span>
                                        <span class="control-label p-l-10">Mini Sidebar</span>
                                    </label>
                                </div>
                            </div>
                            <div class="p-15 border-bottom">
                                <div class="theme-setting-options">
                                    <label class="m-b-0">
                                        <input type="checkbox" name="custom-switch-checkbox" class="custom-switch-input"
                                               id="sticky_header_setting">
                                        <span class="custom-switch-indicator"></span>
                                        <span class="control-label p-l-10">Sticky Header</span>
                                    </label>
                                </div>
                            </div>
                            <div class="mt-4 mb-4 p-3 align-center rt-sidebar-last-ele">
                                <a href="#" class="btn btn-icon icon-left btn-primary btn-restore-theme">
                                    <i class="fas fa-undo"></i> Restore Default
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!-- General JS Scripts -->
    <script src="~/Content/assets/js/app.min.js"></script>
    <script src="~/Content/assets/bundles/izitoast/js/iziToast.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="~/Content/assets/js/page/toastr.js"></script>
    <script src="~/Content/assets/bundles/amcharts4/core.js"></script>
    <script src="~/Content/assets/bundles/amcharts4/charts.js"></script>
    <script src="~/Content/assets/bundles/amcharts4/animated.js"></script>
    <script src="~/Content/assets/bundles/amcharts4/worldLow.js"></script>
    <script src="~/Content/assets/bundles/amcharts4/maps.js"></script>
    <!-- Page Specific JS File -->
    <script src="~/Content/assets/bundles/prism/prism.js"></script>
    <script src="~/Content/assets/js/page/chart-amchart.js"></script>
    <!-- JS Libraies -->
    <script src="~/Content/assets/bundles/apexcharts/apexcharts.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="~/Content/assets/js/page/index.js"></script>
    <!-- Template JS File -->
    <script src="~/Content/assets/js/scripts.js"></script>
    <!-- Custom JS File -->
    <script src="~/Content/assets/js/custom.js"></script>
    <script src="~/Content/assets/bundles/jquery-ui/jquery-ui.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="~/Content/assets/js/page/advance-table.js"></script>
    <script src="~/Content/assets/bundles/sweetalert/sweetalert.min.js"></script>
    <!-- Page Specific JS File -->
    <script src="~/Content/assets/js/page/sweetalert.js"></script>
    @RenderSection("scripts", required: false)
</body>


<!-- index.html  21 Nov 2019 03:47:04 GMT -->
</html>